{
  "name": "timeport",
  "version": "1.0.4",
  "description": "A CLI tool to bridge time tracking between Jira and Harvest",
  "bin": {
    "tp": "./bin/tp.js"
  },
  "type": "module",
  "main": "app.js",
  "scripts": {
    "test": "node --test tests/",
    "test:watch": "node --test --watch tests/",
    "lint": "eslint .",
    "start": "node timeport.js",
    "start:timer": "node bin/tp.js start",
    "update:timer": "node bin/tp.js update",
    "stop:timer": "node bin/tp.js stop"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "dependencies": {
    "chalk": "^5.4.1",
    "dotenv": "^16.4.7",
    "inquirer": "^12.4.2",
    "node-fetch": "^3.3.2",
    "commander": "^11.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/villekivela/timeport.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.5.2"
  }
}
